{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "inputs": [{"local": true, "include": ["."]}],
      "commands": ["cd apps/web && npm install --legacy-peer-deps"]
    },
    "build": {
      "commands": ["cd apps/web && npm run build"]
    }
  },
  "deploy": {
    "startCommand": "cd apps/web && node build/server/index.js"
  }
}
