{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "aptPackages": ["python3", "make", "g++"],
      "commands": ["npm install --legacy-peer-deps"]
    },
    "build": {
      "commands": ["npm run build"]
    }
  },
  "deploy": {
    "startCommand": "node build/server/index.js"
  }
}
